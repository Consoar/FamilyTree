<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bos.whu.familytree">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activitys.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="bos.whu.familytree.ui.activitys.DetailsActivity"/>
        <activity android:name="bos.whu.familytree.ui.activitys.FatherViewActivity"/>
        <activity android:name="bos.whu.familytree.ui.activitys.TreeViewActivity"/>
    </application>

</manifest>
